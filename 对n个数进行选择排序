#include<stdio.h>
void swap(int *a,int *b) 
{
    int temp = *a;
    *a = *b;
    *b = temp;
}

 int main()
 {
    int n;
    scanf("%d",&n);
    int nums[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&nums[i]);
    }
    for(int i=0;i<n-1;i++)
    {
        int index=i;
        for(int j=i+1;j<n;j++)
        {
            if(nums[j]>nums[index]){
                index=j;
            }
        }
        if(index!=i)
        {
            swap(&nums[index],&nums[i]);
        }
    }
    for(int i=0;i<n;i++)
    {
        printf("%d",nums[i]);
        if(i!=n-1){
            printf(" ");
        }
    }
    return 0;
 }
