#include <stdio.h>

struct stu
{
    int sex;
    char name[20];
};

int main()
{
    int n;
    scanf("%d", &n);
    struct stu sudents[n];
    for (int i = 0; i < n; i++)
    {
        scanf("%d %s", &sudents[i].sex, sudents[i].name);    

    }
    int groups[n/2][2];
    int flag[n];
    for (int i = 0; i < n; i++) {
        flag[i] = 0;
    }
    for(int i = 0; i < n/2; i++) 
    {
        for(int j=n-1;j>n/2-1;j--)
        {
            if(flag[j]==0 && sudents[j].sex!=sudents[i].sex)
            {
                groups[i][0]=i;
                groups[i][1]=j;
                flag[i]=1;
                flag[j]=1;
                break;
            }
        }
    }
    for(int i=0;i<n/2;i++)
    {
        printf("%s %s\n",sudents[groups[i][0]].name,sudents[groups[i][1]].name);
    }   
    return 0; 
}  
