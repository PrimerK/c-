#include<stdio.h>
#include<math.h>
int checkprime(int num)
{
    if (num==1)return 0;
    if (num==2)return 1;
    for(int i=2;i<=sqrt(num);i++)
    {
        if(num%i==0)return 0;
    }
    return 1;
}

int main()
{
    int n;
    scanf("%d",&n);
    int arr[n];
    int p=0;
    int cnt=0;
    for(int i=2;i<=n;i++){
        if(checkprime(i)){
            arr[p]=i;
            p++;
        }
    }
    for(int i=0;i+1<p;i++){
        if(arr[i+1]-arr[i]==2){
            cnt+=1;
        }
    }
    printf("%d",cnt);
    return 0;
}
